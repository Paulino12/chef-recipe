      "bundleIdentifier": "com.recipes.platform",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "4855475e-8e1d-42b9-bde0-2ad4f94b2d4b"
      }
    }
  }
}
